<odoo>
   <record id="storewarranty_configuration_model_tree_view" model="ir.ui.view">
      <field name="name">storewarranty.configuration.model.tree.view </field>
      <field name="model">storewarranty.configuration</field>
      <field name="arch" type="xml">
         <tree editable="top" create="1" delete="1" multi_edit="1">
            <field name="name" />
            <field name="product_id" />
            <field name="percentage" />
            <field name="period" />
         </tree>
      </field>
   </record>
</odoo>