<odoo>
    <record id="warranty_product_template_form_view" model="ir.ui.view">
        <field name="name">product.template.warranty.form</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view" />
        <field name="arch" type="xml">
            <group name="description" position="after">
                <group string="Warranty" name="warranty">
                    <field name="is_warranty_available" />
                </group>
            </group>
        </field>
    </record>
</odoo>
