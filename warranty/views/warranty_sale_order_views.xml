<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="sale_order_add_warranty_view" model="ir.ui.view">
        <field name="name">sale_order_add_warranty</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form" />
        <field name="arch" type="xml">
            <xpath expr="//button[@name='action_open_discount_wizard']" position="before">
                <button name="action_open_add_warranty_wizard" type="object" string="Add Warranty" />
            </xpath>
        </field>
    </record>
</odoo>