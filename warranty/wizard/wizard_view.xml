<odoo>
    <record id="action_open_waranty_wizard" model="ir.actions.act_window">
        <field name="name">open.wizard </field>
        <field name="res_model">warranty.wizard</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record id="add_warranty_tree_view" model="ir.ui.view">
        <field name="name">add.warranty.tree.view</field>
        <field name="model">add.warranty.lines</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="product_id" />
                <field name="year" />
                <field name="end_date" />
            </tree>
        </field>

    </record>
</odoo>