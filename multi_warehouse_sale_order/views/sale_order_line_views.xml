<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="multi_warehouse_sale_order_order_line_views" model="ir.ui.view">
        <field name="name">sale.order.form.inherit.multi.warehouse.sale.order.line</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//list/field[@name='price_unit']" position="before" >
                <field name="warehouse_id" optional="show" />
            </xpath>
        </field>
    </record>
</odoo>
