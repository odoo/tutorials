<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="product_template_form_view_inherit_mrp_modular_type">
        <field name="name">product.template.view.form.inherit.mrp.modular.type</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="stock.view_template_property_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='lot_valuated']" position="after">
                <field name="modular_type_ids" widget="many2many_tags"></field>
            </xpath>
        </field>
    </record>
</odoo>
