<?xml version="1.0"?>
<odoo>
<record id="ir_cron_estate_auction_accept" model="ir.cron">
    <field name="name">Auto accept highest auction offer</field>
    <field name="model_id" ref="estate.model_estate_property"/>
    <field name="state">code</field>
    <field name="code">model.check_auction_over()</field>
    <field name="interval_number">5</field>
    <field name="interval_type">minutes</field>
    <field name="active" eval="True"/>
</record>
</odoo>
