<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="partner_shubham" model="res.partner">
        <field name="name">Shubham Agarwal</field>
    </record>
    <record id="partner_het" model="res.partner">
        <field name="name">Het Patel</field>
    </record>
    <record id="partner_ronak" model="res.partner">
        <field name="name">Ronak Bharadia</field>
    </record>
    <record id="partner_utsav" model="res.partner">
        <field name="name">Utsav Maru</field>
    </record>

    <!-- Property Types -->
    <record id="property_type_house" model="estate.property.type">
        <field name="name">House</field>
        <field name="sequence">1</field>
    </record>
    <record id="property_type_apartment" model="estate.property.type">
        <field name="name">Apartment</field>
        <field name="sequence">2</field>
    </record>
    <record id="property_type_villa" model="estate.property.type">
        <field name="name">Villa</field>
        <field name="sequence">3</field>
    </record>
    <record id="property_type_penthouse" model="estate.property.type">
        <field name="name">Penthouse</field>
        <field name="sequence">4</field>
    </record>
    <record id="property_type_cottage" model="estate.property.type">
        <field name="name">Cottage</field>
        <field name="sequence">5</field>
    </record>

    <!-- Property Tags -->
    <record id="property_tag_renovated" model="estate.property.tag">
        <field name="name">Renovated</field>
        <field name="color">2</field>
    </record>
    <record id="property_tag_new" model="estate.property.tag">
        <field name="name">New</field>
        <field name="color">4</field>
    </record>
    <record id="property_tag_furnished" model="estate.property.tag">
        <field name="name">Furnished</field>
        <field name="color">6</field>
    </record>
    <record id="property_tag_pool" model="estate.property.tag">
        <field name="name">Pool</field>
        <field name="color">8</field>
    </record>
    <record id="property_tag_garden" model="estate.property.tag">
        <field name="name">Garden</field>
        <field name="color">10</field>
    </record>

    <!-- Properties -->
    <record id="property_1" model="estate.property">
        <field name="name">Luxury Villa with Pool</field>
        <field name="property_type_id" ref="property_type_villa"/>
        <field name="tag_ids" eval="[(6, 0, [ref('property_tag_pool'), ref('property_tag_garden')])]"/>
        <field name="expected_price">500000.00</field>
        <field name="living_area">300</field>
        <field name="bedrooms">4</field>
        <field name="garden">true</field>
        <field name="garden_area">150</field>
        <field name="garden_orientation">south</field>
        <field name="state">new</field>
    </record>

    <record id="property_2" model="estate.property">
        <field name="name">Modern Apartment Downtown</field>
        <field name="property_type_id" ref="property_type_apartment"/>
        <field name="tag_ids" eval="[(6, 0, [ref('property_tag_new'), ref('property_tag_furnished')])]"/>
        <field name="expected_price">250000.00</field>
        <field name="living_area">85</field>
        <field name="bedrooms">2</field>
        <field name="state">offer_received</field>
    </record>

    <record id="property_3" model="estate.property">
        <field name="name">Cozy Cottage</field>
        <field name="property_type_id" ref="property_type_cottage"/>
        <field name="tag_ids" eval="[(6, 0, [ref('property_tag_renovated')])]"/>
        <field name="expected_price">180000.00</field>
        <field name="living_area">70</field>
        <field name="bedrooms">2</field>
        <field name="garden">true</field>
        <field name="garden_area">100</field>
        <field name="garden_orientation">east</field>
        <field name="state">new</field>
    </record>

    <record id="property_4" model="estate.property">
        <field name="name">Spacious Family House</field>
        <field name="property_type_id" ref="property_type_house"/>
        <field name="tag_ids" eval="[(6, 0, [ref('property_tag_garden')])]"/>
        <field name="expected_price">450000.00</field>
        <field name="living_area">200</field>
        <field name="bedrooms">5</field>
        <field name="garden">true</field>
        <field name="garden_area">200</field>
        <field name="garden_orientation">west</field>
        <field name="state">new</field>
    </record>

    <record id="property_5" model="estate.property">
        <field name="name">Luxury Penthouse</field>
        <field name="property_type_id" ref="property_type_penthouse"/>
        <field name="tag_ids" eval="[(6, 0, [ref('property_tag_new'), ref('property_tag_furnished')])]"/>
        <field name="expected_price">800000.00</field>
        <field name="living_area">150</field>
        <field name="bedrooms">3</field>
        <field name="state">new</field>
    </record>

    <record id="property_6" model="estate.property">
        <field name="name">Beach House</field>
        <field name="property_type_id" ref="property_type_house"/>
        <field name="tag_ids" eval="[(6, 0, [ref('property_tag_pool'), ref('property_tag_furnished')])]"/>
        <field name="expected_price">600000.00</field>
        <field name="living_area">180</field>
        <field name="bedrooms">4</field>
        <field name="state">new</field>
    </record>

    <record id="property_7" model="estate.property">
        <field name="name">City View Apartment</field>
        <field name="property_type_id" ref="property_type_apartment"/>
        <field name="tag_ids" eval="[(6, 0, [ref('property_tag_renovated')])]"/>
        <field name="expected_price">320000.00</field>
        <field name="living_area">95</field>
        <field name="bedrooms">2</field>
        <field name="state">new</field>
    </record>

    <record id="property_8" model="estate.property">
        <field name="name">Mountain Cottage</field>
        <field name="property_type_id" ref="property_type_cottage"/>
        <field name="tag_ids" eval="[(6, 0, [ref('property_tag_garden')])]"/>
        <field name="expected_price">220000.00</field>
        <field name="living_area">90</field>
        <field name="bedrooms">3</field>
        <field name="garden">true</field>
        <field name="garden_area">150</field>
        <field name="garden_orientation">north</field>
        <field name="state">new</field>
    </record>

    <record id="property_9" model="estate.property">
        <field name="name">Suburban Villa</field>
        <field name="property_type_id" ref="property_type_villa"/>
        <field name="tag_ids" eval="[(6, 0, [ref('property_tag_pool'), ref('property_tag_garden')])]"/>
        <field name="expected_price">550000.00</field>
        <field name="living_area">250</field>
        <field name="bedrooms">4</field>
        <field name="garden">true</field>
        <field name="garden_area">300</field>
        <field name="garden_orientation">south</field>
        <field name="state">new</field>
    </record>

    <record id="property_10" model="estate.property">
        <field name="name">Studio Apartment</field>
        <field name="property_type_id" ref="property_type_apartment"/>
        <field name="tag_ids" eval="[(6, 0, [ref('property_tag_furnished')])]"/>
        <field name="expected_price">150000.00</field>
        <field name="living_area">45</field>
        <field name="bedrooms">1</field>
        <field name="state">new</field>
    </record>

    <!-- Property Offers -->
    <record id="offer_1" model="estate.property.offer">
        <field name="property_id" ref="property_1"/>
        <field name="partner_id" ref="partner_utsav"/>
        <field name="price">480000.00</field>
    </record>

    <record id="offer_2" model="estate.property.offer">
        <field name="property_id" ref="property_2"/>
        <field name="partner_id" ref="partner_shubham"/>
        <field name="price">240000.00</field>
        <field name="status">accepted</field>
    </record>

    <record id="offer_3" model="estate.property.offer">
        <field name="property_id" ref="property_5"/>
        <field name="partner_id" ref="partner_ronak"/>
        <field name="price">750000.00</field>
    </record>

    <record id="offer_4" model="estate.property.offer">
        <field name="property_id" ref="property_1"/>
        <field name="partner_id" ref="partner_het"/>
        <field name="price">460000.00</field>
    </record>

    <record id="offer_5" model="estate.property.offer">
        <field name="property_id" ref="property_7"/>
        <field name="partner_id" ref="partner_utsav"/>
        <field name="price">310000.00</field>
    </record>
</odoo>
