<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        
        <!-- Property Types -->
        <record id="estate_property_type_apartment" model="estate.property.type">
            <field name="name">Apartment</field>
        </record>

        <record id="estate_property_type_villa" model="estate.property.type">
            <field name="name">Villa</field>
        </record>

        <record id="estate_property_type_house" model="estate.property.type">
            <field name="name">House</field>
        </record>

        <!-- Property Tags -->
        <record id="estate_tag_modern" model="estate.property.tag">
            <field name="name">Modern</field>
        </record>

        <record id="estate_tag_luxury" model="estate.property.tag">
            <field name="name">Luxury</field>
        </record>

        <record id="estate_tag_family" model="estate.property.tag">
            <field name="name">Family Home</field>
        </record>

        <record id="estate_tag_investment" model="estate.property.tag">
            <field name="name">Good Investment</field>
        </record>

        <!-- Estate Property Records -->
        <record id="1" model="estate.property">
            <field name="name">Modern Apartment</field>
            <field name="description">A beautiful 2-bedroom apartment in the city center.</field>
            <field name="postcode">123456</field>
            <field name="date_availability">2025-05-01</field>
            <field name="expected_price">250000</field>
            <field name="selling_price">0</field>
            <field name="bedrooms">2</field>
            <field name="living_area">80</field>
            <field name="facades">1</field>
            <field name="garage">True</field>
            <field name="garden">False</field>
            <field name="garden_area">0</field>
            <field name="state">new</field>
            <field name="property_type_id" ref="estate_property_type_apartment"/>
            <field name="tag_ids" eval="[(4, ref('estate_tag_modern'))]"/>
            <field name="partner_id" ref="base.user_admin"/>
        </record>
        
        <record id="2" model="estate.property">
            <field name="name">Luxury Villa</field>
            <field name="description">A high-end villa with a private swimming pool.</field>
            <field name="postcode">90210</field>
            <field name="date_availability">2025-06-15</field>
            <field name="expected_price">750000</field>
            <field name="selling_price">0</field>
            <field name="bedrooms">5</field>
            <field name="living_area">300</field>
            <field name="facades">4</field>
            <field name="garage">True</field>
            <field name="garden">True</field>
            <field name="garden_area">200</field>
            <field name="garden_orientation">south</field>
            <field name="state">offer_received</field>
            <field name="property_type_id" ref="estate_property_type_villa"/>
            <field name="tag_ids" eval="[(4, ref('estate_tag_luxury'))]"/>
            <field name="partner_id" ref="base.user_admin"/>
        </record>
        
        <record id="3" model="estate.property">
            <field name="name">Cozy Family House</field>
            <field name="description">A perfect home for a small family in a quiet neighborhood.</field>
            <field name="postcode">456789</field>
            <field name="date_availability">2025-04-10</field>
            <field name="expected_price">350000</field>
            <field name="selling_price">0</field>
            <field name="bedrooms">3</field>
            <field name="living_area">150</field>
            <field name="facades">2</field>
            <field name="garage">False</field>
            <field name="garden">True</field>
            <field name="garden_area">50</field>
            <field name="garden_orientation">east</field>
            <field name="state">offer_accepted</field>
            <field name="property_type_id" ref="estate_property_type_house"/>
            <field name="tag_ids" eval="[(4, ref('estate_tag_family')), (4, ref('estate_tag_investment'))]"/>
            <field name="partner_id" ref="base.user_admin"/>
        </record>
        
        <record id="4" model="estate.property">
            <field name="name">Beachfront Bungalow</field>
            <field name="description">A relaxing bungalow with a stunning ocean view.</field>
            <field name="postcode">808080</field>
            <field name="date_availability">2025-07-20</field>
            <field name="expected_price">500000</field>
            <field name="selling_price">0</field>
            <field name="bedrooms">4</field>
            <field name="living_area">200</field>
            <field name="facades">3</field>
            <field name="garage">False</field>
            <field name="garden">True</field>
            <field name="garden_area">100</field>
            <field name="garden_orientation">west</field>
            <field name="state">new</field>
            <field name="property_type_id" ref="estate_property_type_villa"/>
            <field name="tag_ids" eval="[(4, ref('estate_tag_luxury'))]"/>
            <field name="partner_id" ref="base.user_admin"/>
        </record>
        
        <record id="5" model="estate.property">
            <field name="name">Urban Loft</field>
            <field name="description">A stylish and compact loft in a prime city location.</field>
            <field name="postcode">111222</field>
            <field name="date_availability">2025-08-15</field>
            <field name="expected_price">180000</field>
            <field name="selling_price">0</field>
            <field name="bedrooms">1</field>
            <field name="living_area">60</field>
            <field name="facades">1</field>
            <field name="garage">False</field>
            <field name="garden">False</field>
            <field name="garden_area">0</field>
            <field name="state">new</field>
            <field name="property_type_id" ref="estate_property_type_apartment"/>
            <field name="tag_ids" eval="[(4, ref('estate_tag_modern'))]"/>
            <field name="partner_id" ref="base.user_admin"/>
        </record>

        <record id="6" model="estate.property">
            <field name="name">Mountain Retreat</field>
            <field name="description">A peaceful retreat in the mountains with breathtaking views.</field>
            <field name="postcode">555777</field>
            <field name="date_availability">2025-09-10</field>
            <field name="expected_price">400000</field>
            <field name="selling_price">0</field>
            <field name="bedrooms">4</field>
            <field name="living_area">220</field>
            <field name="facades">3</field>
            <field name="garage">True</field>
            <field name="garden">True</field>
            <field name="garden_area">150</field>
            <field name="garden_orientation">north</field>
            <field name="state">offer_received</field>
            <field name="property_type_id" ref="estate_property_type_house"/>
            <field name="tag_ids" eval="[(4, ref('estate_tag_family')), (4, ref('estate_tag_investment'))]"/>
            <field name="partner_id" ref="base.user_admin"/>
        </record>
        
        <record id="7" model="estate.property">
            <field name="name">Downtown Penthouse</field>
            <field name="description">A luxurious penthouse with a panoramic city view.</field>
            <field name="postcode">333999</field>
            <field name="date_availability">2025-10-05</field>
            <field name="expected_price">900000</field>
            <field name="selling_price">0</field>
            <field name="bedrooms">3</field>
            <field name="living_area">180</field>
            <field name="facades">2</field>
            <field name="garage">True</field>
            <field name="garden">False</field>
            <field name="garden_area">0</field>
            <field name="state">offer_accepted</field>
            <field name="property_type_id" ref="estate_property_type_apartment"/>
            <field name="tag_ids" eval="[(4, ref('estate_tag_luxury')), (4, ref('estate_tag_modern'))]"/>
            <field name="partner_id" ref="base.user_admin"/>
        </record>
    </data>
</odoo>
