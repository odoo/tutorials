<?xml version="1.0"?>
<odoo>
     <record id="estate_property_offers_report_print" model="ir.actions.report">
        <field name="name">Print</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.estate_property_offers_report_template</field>
        <field name="report_file">estate.estate_property_offers_report_template</field>
        <field name="print_report_name">'Property Offers - %s' %(object.name or 'name').replace('/','')</field>
        <field name="binding_model_id" ref="model_estate_property" />
        <field name="binding_type">report</field>
    </record>
    <record id="estate_res_user_offer_report_print" model="ir.actions.report">
        <field name="name">Print</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.res_user_offer_report_template</field>
        <field name="report_file">estate.res_user_offer_report_template</field>
        <field name="print_report_name">'Salesman - %s' %(object.name or 'name').replace('/','')</field>
        <field name="binding_model_id" ref="base.model_res_users" />
        <field name="binding_type">report</field>
    </record>
</odoo>