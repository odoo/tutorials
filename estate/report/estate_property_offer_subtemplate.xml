<odoo>
    <template id="estate_property_subtemplate">
        <table class="table">
            <thead>
                <tr>
                    <th>Price</th>
                    <th>Partner</th>
                    <th>Validity</th>
                    <th>Deadline</th>
                    <th>State</th>
                </tr>
            </thead>
            <tbody>
                <t t-foreach="offers" t-as="offer">
                    <tr>
                        <td>
                            <span t-out="offer.price" />
                        </td>
                        <td>
                            <span t-out="offer.partner_id.name" />
                        </td>
                        <td>
                            <span t-out="offer.validity" />
                        </td>
                        <td>
                            <span t-out="offer.date_deadline" />
                        </td>
                        <td>
                            <t t-if="offer.status">
                                <span t-out="offer.status" />
                            </t>
                            <t t-else="">
                                <span t-out="offer.status" />
                            </t>
                        </td>
                    </tr>
                </t>
            </tbody>
        </table>
    </template>
</odoo>
