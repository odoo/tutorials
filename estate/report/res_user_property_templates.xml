<odoo>
    <template id="report_properties_table">
        <div>
            <h3>Properties</h3>
            <table class="table">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Expected Price</th>
                        <th>State</th>
                    </tr>
                </thead>
                <tbody>
                    <tr t-foreach="properties" t-as="property">
                        <td>
                            <span t-field="property.name" />
                        </td>
                        <td>
                            <span t-field="property.expected_price" />
                        </td>
                        <td>
                            <span t-field="property.state" />
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </template>
</odoo>