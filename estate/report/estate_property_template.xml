<odoo>
    <templates>
        <t t-name="estate_property.property_report">
            <t t-call="web.html_container">
                <t t-call="web.external_layout">
                    <div class="page">
                        <h2>
                            <span t-field="name" />
                        </h2>
                        <div>
                            <strong>Expected Price: </strong>
                            <span t-field="expected_price" />
                            <strong>Selling Price: </strong>
                            <span t-field="selling_price" />
                        </div>
                        <table class="table table-striped">
                            <thead>
                                <tr>
                                    <th>Price</th>
                                    <th>Partner</th>
                                    <th>Validity (days)</th>
                                    <th>Deadline</th>
                                    <th>Status</th>
                                </tr>
                            </thead>
                            <tbody>
                                <t t-foreach="offer_ids" t-as="offer">
                                    <tr>
                                        <td>
                                            <t t-out="offer.price"/>
                                        </td>
                                        <td>
                                            <t t-out="offer.partner_id.name"/>  <!-- Assuming you want the partner's name -->
                                        </td>
                                        <td>
                                            <t t-out="offer._validity"/>
                                        </td>
                                        <td>
                                            <t t-out="offer.deadline"/>
                                        </td>
                                        <td>
                                            <t t-out="offer.status"/>
                                        </td>
                                    </tr>
                                </t>
                            </tbody>
                        </table>
                    </div>
                </t>
            </t>
        </t>
    </templates>
</odoo>
