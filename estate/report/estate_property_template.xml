<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="estate_properties" name="All estate properties">
        <t t-call="website.layout">
            <div class="w-100 bg-danger d-flex justify-content-center align-item-center">
                <div class="bg-success">
                    <h1>Estate Properties</h1>
                    <t t-foreach="properties" t-as="property">
                        <t t-out="property.name"/>
                    </t>
                </div>
            </div>
        </t>
    </template>
</odoo>
