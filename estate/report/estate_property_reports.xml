<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="report_estate_property_offer" model="ir.actions.report">
        <field name="name">Print Property</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.estate_property_offer_report_template</field>
        <field name="report_file">estate.estate_property_offer_report_template</field>
        <field name="print_report_name">'Property -%s' % (object.name or 'Attendee').replace('/','')</field>
        <field name="binding_model_id" ref="model_estate_property"/>
        <field name="binding_type">report</field>
    </record>

    <record id="report_salesman_property_detail" model="ir.actions.report">
        <field name="name">Print Properties</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.salesman_property_details_report_template</field>
        <field name="report_file">estate.salesman_property_details_report_template</field>
        <field name="print_report_name">'Property Details'</field>
        <field name="binding_model_id" ref="model_res_users"/>
        <field name="binding_type">report</field>
    </record>
</odoo>
