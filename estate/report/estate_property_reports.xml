<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="report_property_offers" model="ir.actions.report">
        <field name="name">Property Offers Report</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.estate_property_offers_report</field>
        <field name="report_file">estate.estate_property_offers_report</field>
        <field name="print_report_name">'Property Offers - %s' % (object.name or 'Property').replace('/','')</field>
        <field name="binding_model_id" ref="model_estate_property"/>
        <field name="binding_type">report</field>
    </record>   
    <record id="report_salesman_property" model="ir.actions.report">
        <field name="name">Salesman Property Report</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.estate_salesman_property_report</field>
        <field name="report_file">estate.estate_salesman_property_report</field>
        <field name="print_report_name">'Property Offers - %s' % (object.name or 'Salesman Property').replace('/','')</field>
        <field name="binding_model_id" ref="model_res_users"/>
        <field name="binding_type">report</field>
    </record>
</odoo>
