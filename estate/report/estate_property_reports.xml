<odoo>
    <record id="action_report_property_offer_list" model="ir.actions.report">
        <field name="name">Property Offer List</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.estate_property_report_template</field>
        <field name="report_file">estate.estate_property_report_template</field>
        <field name="binding_model_id" ref="estate.model_estate_property"/>
        <field name="binding_type">report</field>
        <field name="print_report_name">'Properties_Offers_%s' % (object.name)</field>
    </record>
    <record id="action_report_salesperson_property_offer_list" model="ir.actions.report">
        <field name="name">Salesman Property Offer List</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.estate_salesman_property_report_template</field>
        <field name="report_file">estate.estate_salesman_property_report_template</field>
        <field name="binding_model_id" ref="base.model_res_users"/>
        <field name="binding_type">report</field>
        <field name="print_report_name">'Salesman_Properties_Offers_%s' % (object.name)</field>
    </record>
</odoo>
