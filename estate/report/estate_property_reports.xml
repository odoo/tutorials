<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="action_report_estate_property" model="ir.actions.report">
        <field name="name">Print Property Report</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_property_offers</field>    
        <field name="print_report_name">'%s' % object.name</field>
        <field name="binding_model_id" ref="model_estate_property"/>
        <field name="binding_type">report</field>
    </record>

    <record id="action_report_res_user" model="ir.actions.report">
        <field name="name">Salesman reports</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_res_user</field>
        <field name="print_report_name">'%s' % object.name</field>
        <field name="binding_model_id" ref="model_res_users"/>
        <field name="binding_type">report</field>
    </record>

</odoo>
