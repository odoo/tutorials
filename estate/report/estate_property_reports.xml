<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="action_estate_property_report" model="ir.actions.report">
        <field name="name">Print</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_property_offers</field>
        <field name="print_report_name">'Property_Offers_%s' % (object.name)</field>
        <field name="binding_model_id" ref="estate.model_estate_property" />
        <field name="binding_type">report</field>
    </record>

    <record id="action_estate_property_user_report" model="ir.actions.report">
        <field name="name">Print</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_property_user_offer</field>
        <field name="print_report_name">'Property_Offers_%s' % (object.name)</field>
        <field name="binding_model_id" ref="base.model_res_users" />
        <field name="binding_type">report</field>
    </record>
</odoo>
