<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="report_estate_porperty_offer" model="ir.actions.report">
        <field name="name">Property Offers</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_property_offers</field>
        <field name="report_file">event.report_property_offers</field>
        <field name="print_report_name">'Property Offers - %s' % (object.name or 'Property 1')</field>
        <field name="binding_model_id" ref="model_estate_property"/>
        <field name="binding_type">report</field>
    </record>

    <record id="report_estate_porperty_salesman_properties" model="ir.actions.report">
        <field name="name">Salesman Properties</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_salesman_property</field>
        <field name="report_file">event.report_salesman_property</field>
        <field name="print_report_name">'Salesman Properties- %s' % (object.name or 'Property 1')</field>
        <field name="binding_model_id" ref="model_res_users"/>
        <field name="binding_type">report</field>
    </record>
</odoo>
