<odoo>
    <record id="estate_property_offers_report" model="ir.actions.report">
        <field name="name">Property Offers</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_file">estate.report_property_offers</field>
        <field name="report_name">estate.report_property_offers</field>
        <field name="print_report_name">'Offers Report - %s' % (object.name or 'offers')</field>
        <field name="binding_model_id" ref="estate.model_estate_property"/>
        <field name="binding_type">report</field>
    </record>
    <record id="salesman_property_offers_report" model="ir.actions.report">
        <field name="name">Property Offers</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_file">estate.report_salesman_offers</field>
        <field name="report_name">estate.report_salesman_offers</field>
        <field name="print_report_name">'Offers Report - %s' % (object.name or 'offers')</field>
        <field name="binding_model_id" ref="base.model_res_users"/>
        <field name="binding_type">report</field>
    </record>
</odoo>
