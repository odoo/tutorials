<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record id="estate_property_report" model="ir.actions.report">
            <field name="name">Property Offer</field>
            <field name="model">estate.property</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">estate.estate_property_template</field>
            <field name="report_file">estate.report_property</field>
            <field name="print_report_name">'%s Report' % object.name</field>
            <field name="binding_model_id" ref="model_estate_property" />
            <field name="binding_type">report</field>
        </record>
    </data>

    <data>
        <record id="estate_property_users_report" model="ir.actions.report">
            <field name="name">Property Offers</field>
            <field name="model">res.users</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">estate.res_users_estate_property_template</field>
            <field name="print_report_name">'%s Report' % object.name</field>
            <field name="print_report_name">'Salesman Property Offers'</field>
            <field name="binding_model_id" ref="base.model_res_users"/>
            <field name="binding_type">report</field>
        </record>
    </data>
</odoo>
