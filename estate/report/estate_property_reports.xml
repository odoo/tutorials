<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    
    <record id="estate_property_report" model="ir.actions.report">
        <field name="name"> Property</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-html</field>
        <field name="report_name">estate.estate_property_report_template</field>
        <field name="report_file">estate.estate_property_report_template</field>
        <field name="print_report_name">'Estate Property - %s' % (object.name or 'Property').replace('/','')</field>
        <field name="binding_model_id" ref="model_estate_property"/>
        <field name="binding_type">report</field>
    </record>

    <record id="user_estate_property_report" model="ir.actions.report">
        <field name="name">Real Estate Property</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-html</field>
        <field name="report_name">estate.user_estate_property_report_template</field>
        <field name="report_file">estate.user_estate_property_report_template</field>
        <field name="print_report_name">'Estate Property - %s' % (object.name or 'Real Estate Property').replace('/','')</field>
        <field name="binding_model_id" ref="base.model_res_users"/>
        <field name="binding_type">report</field>
    </record>

</odoo>