<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="report_estate_property" model="ir.actions.report">
        <field name="name">Print</field>
        <field name="model">estate.property</field>
        <field name="report_name">estate.report_property_offers</field>
        <field name="report_file">estate.report_property_offers</field>
        <field name="print_report_name">'%s_offers' % (object.name or 'Property')</field>
        <field name="binding_model_id" ref="model_estate_property" />
        <field name="binding_type">report</field>
        <field name="report_type">qweb-pdf</field>

    </record>

    <record id="report_estate_property_res_user" model="ir.actions.report">
        <field name="name">Report</field>
        <field name="model">res.users</field>
        <field name="report_name">estate.estate_property_offer_res_user_template</field>
        <field name="report_file">estate.estate_property_offer_res_user_template</field>
        <field name="binding_model_id" ref="model_res_users" />
        <field name="binding_type">report</field>
        <field name="report_type">qweb-pdf</field>
    </record>
</odoo>
