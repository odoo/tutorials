<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="property_offer_report" model="ir.actions.report">
        <field name="name">Property Offer</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-html</field>
        <field name="report_name">estate.estate_property_offer_template</field>
        <field name="report_file">estate.estate_property_offer_template</field>
        <field name="print_report_name">' %s property offers' % (object.name)</field>
        <field name="binding_model_id" ref="model_estate_property"></field>
        <field name="binding_type">report</field>
    </record>

    <record id="property_resusers_offer_report" model="ir.actions.report">
        <field name="name">users Offers</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-html</field>
        <field name="report_name">estate.test_user_offer_template</field>
        <field name="report_file">estate.test_user_offer_template</field>
        <field name="print_report_name">' %s property offers' % (object.name)</field>
        <field name="binding_model_id" ref="base.model_res_users"/>
        <field name="binding_type">report</field>
    </record>
</odoo>
