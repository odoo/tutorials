<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="action_property_offer_report" model="ir.actions.report">
        <field name="name">Property Offers Report</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_property_offers</field>
        <field name="report_file">estate.report_property_offers</field>
        <field name="print_report_name">'Property%s' % (object.name)</field>
        <field name="binding_model_id" ref="model_estate_property"/>
        <field name="binding_type">report</field>
    </record>

    <record id="action_user_property_report" model="ir.actions.report">
        <field name="name">User Property Report</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_user_properties</field>
        <field name="report_file">estate.report_user_properties</field>
        <field name="print_report_name">'Property2%s' % (object.name)</field>
        <field name="binding_model_id" ref="base.model_res_users"/>
        <field name="binding_type">report</field>
        <field name="domain">[]</field>
    </record>
</odoo>