<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="report_estate_property_offers" model="ir.actions.report">
        <field name="name">Property Offers</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_property_offers</field>
        <field name="report_file">estate.report_property_offers</field>
        <field name="print_report_name">'Property - %s' % (object.name or object.reference).replace('/','')</field>
        <field name="binding_model_id" ref="model_estate_property"/>
        <field name="binding_type">report</field>
    </record>
    <record id="report_user_estate_property" model="ir.actions.report">
        <field name="name">User Estate Property Report</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.user_estate_property_report</field>
        <field name="report_file">estate.user_estate_property_report</field>
        <field name="print_report_name">'User Estate Property - {}'.format(object.name or 'Attendee').replace('/','')</field>
        <field name="binding_model_id" ref="model_res_users"/>
        <field name="binding_type">report</field>
    </record>
</odoo>
