<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Property Offers Report -->
    <record id="report_property_offers" model="ir.actions.report">
        <field name="name">Property Offers Report</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.property_offers_report_template</field>
        <field name="binding_model_id" ref="estate.model_estate_property"/>
    </record>

    <!-- User Real Estate Report -->
    <record id="report_res_users_estate" model="ir.actions.report">
        <field name="name">Salesman Estate Report</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.res_users_estate_report_template</field>
        <field name="binding_model_id" ref="base.model_res_users"/>
    </record>
</odoo>
