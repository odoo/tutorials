<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="action_report_estate_property" model="ir.actions.report">
        <field name="name">Print Details</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_property_offers</field>
        <field name="print_report_name">'%s' % object.name</field>
        <field name="binding_model_id" ref="model_estate_property"/>
        <field name="binding_type">report</field>
    </record>

    <record id="action_report_res_users" model="ir.actions.report">
        <field name="name">Salesman Report</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_seller_properties</field>
        <field name="report_file">report.name</field>
        <field name="print_report_name">'%s' % object.name</field>
        <field name="binding_model_id" ref="base.model_res_users"/>
        <field name="binding_type">report</field>
    </record>

</odoo>
