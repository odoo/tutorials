<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="report_property_offers" model="ir.actions.report">
        <field name="name">Property Offers</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-html</field>
        <field name="report_name">estate.estate_property_template</field>
        <field name="print_report_name">'%s Report' % object.name</field>
        <field name="paperformat_id" ref="base.paperformat_euro" />
        <field name="binding_model_id" ref="estate.model_estate_property" />
        <field name="binding_type">report</field>
    </record>
    <record id="estate_property_users_report" model="ir.actions.report">
        <field name="name">Properties Report</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.res_users_estate_property_template</field>
        <field name="print_report_name">'%s Report' % object.name</field>
        <field name="binding_model_id" ref="model_res_users" />
        <field name="binding_type">report</field>
    </record>
</odoo>
