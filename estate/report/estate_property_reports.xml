<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="report_estate_property" model="ir.actions.report">
        <field name="name">Print</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.estate_property_report_main_template</field>
        <field name="report_file">estate.estate_property_report_main_template</field>
        <field name="print_report_name">'Property Report - %s' % (object.name or 'Attendee').replace('/','')</field>
        <field name="binding_model_id" ref="model_estate_property"/>
        <field name="binding_type">report</field>
    </record>

</odoo>
