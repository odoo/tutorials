<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="report_estate_property" model="ir.actions.report">
        <field name="name">Property Report</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.property_report_template</field>
        <field name="report_file">estate.property_report_template</field>
        <field name="print_report_name">'%s Report' % (object.name).replace('/','')</field>
        <field name="binding_model_id" ref="model_estate_property" />
        <field name="binding_type">report</field>
    </record>

    <record id="report_salesperson_property" model="ir.actions.report">
        <field name="name">Property Report</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.salesperson_property_report_template</field>
        <field name="report_file">estate.salesperson_property_report_template</field>
        <field name="print_report_name">'%s Report' % (object.name).replace('/','')</field>
        <field name="paperformat_id" ref="base.paperformat_euro" />
        <field name="binding_model_id" ref="base.model_res_users" />
        <field name="binding_type">report</field>
    </record>
</odoo>
