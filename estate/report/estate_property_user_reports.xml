<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="report_user_property" model="ir.actions.report">
        <field name="name">Print Properties</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.user_property_template</field>
        <field name="report_file">estate.user_property_template</field>
        <field name="print_report_name">'Property - %s' % (object.name or 'Attendee').replace('/','')</field>
        <field name="binding_model_id" ref="base.model_res_users"/>
        <field name="binding_type">report</field>
    </record>
</odoo>
