<?xml version="1.0"?>
<odoo>

    <record id="estate_property_tag_model_action" model="ir.actions.act_window">
        <field name="name">Property Tag</field>
        <field name="res_model">estate_property_tag_model</field>
        <field name="view_mode">list,form,kanban,graph</field>
    </record>
    <!-- form view -->
    <record id="estate_property_tag_form_view" model="ir.ui.view">
        <field name="name">estate_property_tag_form_view</field>
        <field name="model">estate_property_tag_model</field>
        <field name="arch" type="xml">
            <form string="Property_Tag_Form">
                <h3>Name</h3>
                <field name="name"/>
            </form>
        </field>
    </record>
</odoo>