<odoo>
    <data noupdate="1"> <record id="property_type_house" model="estate.property.type">
            <field name="name">House</field>
            <field name="sequence">1</field>
        </record>
        <record id="property_type_apartment" model="estate.property.type">
            <field name="name">Apartment</field>
            <field name="sequence">2</field>
        </record>
        <record id="property_type_land" model="estate.property.type">
            <field name="name">Land</field>
            <field name="sequence">3</field>
        </record>

        <record id="property_tag_cozy" model="estate.property.tag">
            <field name="name">Cozy</field>
            <field name="color">2</field> </record>
        <record id="property_tag_renovated" model="estate.property.tag">
            <field name="name">Renovated</field>
            <field name="color">5</field> </record>
        <record id="property_tag_garden" model="estate.property.tag">
            <field name="name">Garden</field>
            <field name="color">7</field> </record>
        <record id="property_tag_spacious" model="estate.property.tag">
            <field name="name">Spacious</field>
            <field name="color">9</field> </record>

        <record id="property_1" model="estate.property">
            <field name="name">Beautiful House near Beach</field>
            <field name="description">A lovely house with 3 bedrooms and a sea view.</field>
            <field name="postcode">90210</field>
            <field name="date_availability" eval="(DateTime.now() + relativedelta(months=1)).strftime('%Y-%m-%d')"/>
            <field name="expected_price">250000.0</field>
            <field name="bedrooms">3</field>
            <field name="living_area">120</field>
            <field name="facades">4</field>
            <field name="garage">True</field>
            <field name="garden">True</field>
            <field name="garden_area">50</field>
            <field name="garden_orientation">S</field>
            <field name="property_type_id" ref="property_type_house"/>
            <field name="tag_ids" eval="[(6, 0, [ref('property_tag_cozy'), ref('property_tag_garden')])]"/>
            </record>

        <record id="property_2" model="estate.property">
            <field name="name">Modern Apartment Downtown</field>
            <field name="description">Stylish apartment in the city center, recently renovated.</field>
            <field name="postcode">10001</field>
            <field name="date_availability" eval="(DateTime.now() + relativedelta(days=15)).strftime('%Y-%m-%d')"/>
            <field name="expected_price">180000.0</field>
            <field name="bedrooms">2</field>
            <field name="living_area">85</field>
            <field name="facades">2</field>
            <field name="garage">False</field>
            <field name="garden">False</field>
            <field name="property_type_id" ref="property_type_apartment"/>
            <field name="tag_ids" eval="[(6, 0, [ref('property_tag_renovated'), ref('property_tag_spacious')])]"/>
            <field name="state">offer_received</field> </record>

        <record id="property_3" model="estate.property">
            <field name="name">Large Plot of Land</field>
            <field name="description">Vast land area suitable for building.</field>
            <field name="postcode">54321</field>
            <field name="date_availability" eval="(DateTime.now() + relativedelta(months=6)).strftime('%Y-%m-%d')"/>
            <field name="expected_price">95000.0</field>
            <field name="living_area">0</field> <field name="property_type_id" ref="property_type_land"/>
            </record>

        <record id="property_4_sold" model="estate.property">
            <field name="name">Charming Cottage (Sold)</field>
            <field name="description">A small, sold cottage.</field>
            <field name="postcode">12345</field>
            <field name="date_availability" eval="(DateTime.now() - relativedelta(months=2)).strftime('%Y-%m-%d')"/> <field name="expected_price">150000.0</field>
            <field name="selling_price">145000.0</field> <field name="bedrooms">2</field>
            <field name="living_area">70</field>
            <field name="property_type_id" ref="property_type_house"/>
            <field name="tag_ids" eval="[(6, 0, [ref('property_tag_cozy')])]"/>
            <field name="state">sold</field> <field name="buyer_id" ref="base.partner_demo"/> </record>

        <record id="offer_1_prop_2" model="estate.property.offer">
            <field name="price">175000.0</field>
            <field name="property_id" ref="property_2"/>
            <field name="partner_id" ref="base.partner_demo"/>
            <field name="validity">10</field>
            </record>

        <record id="offer_2_prop_2" model="estate.property.offer">
            <field name="price">178000.0</field>
            <field name="property_id" ref="property_2"/>
            <field name="partner_id" ref="base.partner_demo_portal"/> <field name="validity">5</field>
             <field name="state">accepted</field> </record>

        <record id="offer_1_prop_1" model="estate.property.offer">
            <field name="price">240000.0</field>
            <field name="property_id" ref="property_1"/>
            <field name="partner_id" ref="base.partner_demo"/>
            <field name="validity">14</field>
            <field name="state">refused</field> </record>

    </data>
</odoo>
