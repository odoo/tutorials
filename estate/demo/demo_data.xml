<odoo>
  <data noupdate="1">
    <record id="estate_property_1" model="estate.property">
        <field name="name">Big Villa</field>
        <field name="state">new</field>
        <field name="description">A nice big villa</field>
        <field name="postcode">12345</field>
        <field name="date_availability">2020-02-02</field>
        <field name="expected_price" eval="1600000"/>
        <field name="selling_price" eval="0"/>
        <field name="bedrooms" eval="6"/>
        <field name="living_area" eval="100"/>
        <field name="facades" eval="4"/>
        <field name="garage" eval="True"/>
        <field name="garden" eval="True"/>
        <field name="garden_area" eval="100000"/>
        <field name="garden_orientation">south</field>
        <field name="property_type_id" ref="estate.estate_property_type_1"/>
    </record>

    <record id="estate_property_2" model="estate.property">
      <field name="name">Trailer Home</field>
      <field name="state">cancelled</field>
      <field name="description">Home in a trailer park</field>
      <field name="postcode">54321</field>
      <field name="date_availability">1970-01-01</field>
      <field name="expected_price" eval="100000"/>
      <field name="selling_price" eval="120000"/>
      <field name="bedrooms" eval="1"/>
      <field name="living_area" eval="10"/>
      <field name="facades" eval="4"/>
      <field name="garage" eval="False"/>
      <field name="garden" eval="False"/>
      <field name="garden_area" eval="0"/>
      <field name="garden_orientation"></field>
      <field name="property_type_id" ref="estate.estate_property_type_1"/>
    </record>

    <record id="estate_property_3" model="estate.property">
      <field name="name">Acorn Field</field>
      <field name="state">new</field>
      <field name="description">Large field of Acorn</field>
      <field name="postcode">67890</field>
      <field name="date_availability">2000-01-01</field>
      <field name="expected_price" eval="100000"/>
      <field name="selling_price" eval="0"/>
      <field name="bedrooms" eval="0"/>
      <field name="living_area" eval="0"/>
      <field name="facades" eval="0"/>
      <field name="garage" eval="False"/>
      <field name="garden" eval="True"/>
      <field name="garden_area" eval="1000"/>
      <field name="garden_orientation"></field>
      <field name="property_type_id" ref="estate.estate_property_type_4"/>
      <field name="property_offer_ids" eval="[
          Command.create({
              'partner_id': ref('base.res_partner_2'),
              'price': 100001.0,
              'validity': 10
          }),
          Command.create({
              'partner_id': ref('base.res_partner_1'),
              'price': 99999.0,
              'validity': 10
          }),
      ]"/>
    </record>

    <record id="estate_property_offer_1" model="estate.property.offer">
      <field name="property_id" ref="estate_property_1"/>
      <field name="partner_id" ref="base.res_partner_2"/>
      <field name="price" eval="10000"/>
      <field name="date_deadline" eval="(datetime.now() + timedelta(days=14))"/>
    </record>
    <record id="estate_property_offer_2" model="estate.property.offer">
      <field name="property_id" ref="estate_property_1"/>
      <field name="partner_id" ref="base.res_partner_2"/>
      <field name="price" eval="1500000"/>
      <field name="date_deadline" eval="(datetime.now() + timedelta(days=14))"/>
    </record>
    <record id="estate_property_offer_3" model="estate.property.offer">
      <field name="property_id" ref="estate_property_1"/>
      <field name="partner_id" ref="base.res_partner_1"/>
      <field name="price" eval="1500001"/>
      <field name="date_deadline" eval="(datetime.now() + timedelta(days=14))"/>
    </record>

    <function model="estate.property.offer" name="accept_offer">
      <value eval="[ref('estate_property_offer_3')]"/>
    </function>

    <function model="estate.property.offer" name="decline_offer">
      <value eval="[ref('estate_property_offer_2'), ref('estate_property_offer_1')]"/>
    </function>
  </data>  
</odoo>