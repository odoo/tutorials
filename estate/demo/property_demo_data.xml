<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- loading property tags -->
        <record id="estate_property_tag_luxury" model="estate.property.tags">
            <field name="name">Luxury</field>
        </record>

        <record id="estate_property_tag_family_friendly" model="estate.property.tags">
            <field name="name">Family Friendly</field>
        </record>

        <record id="estate_property_tag_seaside" model="estate.property.tags">
            <field name="name">Seaside</field>
        </record>

        <record id="estate_property_tag_modern" model="estate.property.tags">
            <field name="name">Modern</field>
        </record>

        <record id="estate_property_tag_rural" model="estate.property.tags">
            <field name="name">Rural</field>
        </record>

        <record id="estate_property_tag_comfort" model="estate.property.tags">
