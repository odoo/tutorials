        </record>

        <record id="estate_property_tag_garden" model="estate.property.tags">
            <field name="name">Garden</field>
        </record>

        <!-- loading property demo data  -->
        <record id="property_demo_data_1" model="estate.property">
            <field name="name">Big Villa</field>
            <field name="state">new</field>
            <field name="description">A nice and big villa</field>
            <field name="postcode">12345</field>
            <field name="date_availability">2020-02-02</field>
            <field name="expected_price">1600000</field>
            <field name="bedrooms">6</field>
            <field name="living_area">100</field>
            <field name="facades">4</field>
            <field name="garage" eval="True" />
            <field name="garden">True</field>
            <field name="garden_area">100000</field>
            <field name="garden_orientation">south</field>
            <field name="property_type_id" ref="property_type_residential" />
            <field name="property_image" type="base64" file="estate/static/scr/img/big_villa.jpg" />
        </record>
        <record id="property_demo_data_2" model="estate.property">
            <field name="salesperson_id" ref="base.user_demo" />
            <field name="name">Trailer home</field>
            <field name="state">cancelled</field>
            <field name="description">Home in a trailer park</field>
            <field name="postcode">54321</field>
            <field name="date_availability">1970-01-01</field>
            <field name="expected_price">100000</field>
            <field name="selling_price">120000</field>
            <field name="bedrooms">1</field>
            <field name="living_area">10</field>
            <field name="facades">4</field>
            <field name="garage" eval="False" />
            <field name="property_type_id" ref="estate.property_type_residential" />
            <field name="property_image" type="base64" file="estate/static/scr/img/trailer_home.jpg" />
        </record>
        <record id="new_property_id" model="estate.property">
            <field name="name">Modern Apartment</field>
            <field name="state">new</field>
            <field name="description">A modern apartment in the city center</field>
            <field name="postcode">67890</field>
            <field name="date_availability">2023-08-15</field>
            <field name="expected_price">2000000</field>
            <field name="bedrooms">4</field>
            <field name="living_area">150</field>
            <field name="facades">2</field>
            <field name="garage" eval="False" />
            <field name="garden">False</field>
            <field name="property_type_id" ref="estate.property_type_industrial" />
            <field name="property_image" type="base64" file="estate/static/scr/img/rene_skyline.jpg" />
            <field name="offer_ids"
                eval="[Command.create({
                'price': 1800000,
                'partner_id': ref('base.res_partner_1'),
                'validity':14,
                'date_deadline': (DateTime.now() + timedelta(days=14))
            })]" />
        </record>
        <record id="estate_property_luxury_apartment" model="estate.property">
            <field name="name">Luxury Apartment</field>
            <field name="state">new</field>
            <field name="description">A luxury apartment located in the heart of the city.</field>
            <field name="postcode">67890</field>
            <field name="date_availability" eval="(DateTime.today() + relativedelta(days=30))" />
            <field name="expected_price">850000</field>
            <field name="bedrooms">3</field>
            <field name="living_area">150</field>
            <field name="facades">2</field>
            <field name="garage" eval="True" />
            <field name="garden">False</field>
            <field name="garden_area">0</field>
            <field name="garden_orientation">north</field>
            <field name="property_type_id" ref="estate.property_type_residential" />
            <field name="tag_ids" eval="[(6, 0, [ref('estate_property_tag_luxury')])]" />
            <field name="property_image" type="base64"
                file="estate/static/scr/img/luxury_apartment.jpg" />
        </record>

        <record id="estate_property_seaside_cottage" model="estate.property">
            <field name="name">Seaside Cottage</field>
