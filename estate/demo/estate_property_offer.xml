<odoo>
    <record id="demo_offer_1" model="estate.property.offer">
        <field name="partner_id" ref="base.res_partner_12" />
        <field name="property_id" ref="demo_property_1" />
        <field name="price">10000</field>
        <field name="date_deadline" eval="datetime.today()+timedelta(days=30)" />
    </record>

    <record id="demo_offer_2" model="estate.property.offer">
        <field name="partner_id" ref="base.res_partner_12" />
        <field name="property_id" ref="demo_property_1" />
        <field name="price">1500000</field>
        <field name="date_deadline" eval="datetime.today()+timedelta(days=30)" />
    </record>

    <record id="demo_offer_3" model="estate.property.offer">
        <field name="partner_id" ref="base.res_partner_2" />
        <field name="property_id" ref="demo_property_1" />
        <field name="price">1505000</field>
        <field name="date_deadline" eval="datetime.today()+timedelta(days=15)" />
    </record>

    <function model="estate.property.offer" name="action_accept">
        <value eval="[ref('demo_offer_3')]" />
    </function>
</odoo>
