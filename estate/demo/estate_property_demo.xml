        <record id="estate_property_rene_skyline" model="estate.property">
            <field name="name">Rene Skyline</field>
            <field name="state">offer_received</field>
            <field name="description">Touch the clouds</field>
            <field name="postcode">54321</field>
            <field name="date_availability" eval="(DateTime.today() + relativedelta(months=3))" />
