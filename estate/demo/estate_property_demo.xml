            <field name="offer_ids" eval="[
                Command.create({
                    'partner_id': ref('base.res_partner_12'),
                    'price': 98000,
                    'validity': 14,
                }),
                Command.create({
                    'partner_id': ref('base.res_partner_3'),
                    'price': 105000,
                    'validity': 14,
                }),
                Command.create({
                    'partner_id': ref('base.res_partner_1'),
                    'price': 110000,
                    'validity': 14,
                }),
            ]" />
        </record>

