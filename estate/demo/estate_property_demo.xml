<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="estate_property_big_villa" model="estate.property">
            <field name="name">Big Villa</field>
            <field name="state">new</field>
            <field name="image" type="base64" file="estate/static/src/img/image1.jpg" />
            <field name="description">A nice and big villa</field>
            <field name="postcode">123456</field>
            <field name="date_availability">2020-02-02</field>
            <field name="expected_price">1600000</field>
            <field name="bedrooms">6</field>
            <field name="living_area">100</field>
            <field name="facades">4</field>
            <field name="is_garage">True</field>
            <field name="is_garden">True</field>
            <field name="garden_area">100000</field>
            <field name="garden_orientation">south</field>
            <field name="property_type_id" ref="estate.estate_property_residential" />
        </record>
        <record id="estate_property_trailer" model="estate.property">
            <field name="name">Trailer Home</field>
            <field name="image" type="base64" file="estate/static/src/img/image2.jpg" />
            <field name="state">cancelled</field>
            <field name="description">Home in a trailer park</field>
            <field name="postcode">654321</field>
            <field name="date_availability">1970-01-01</field>
            <field name="expected_price">100000</field>
            <field name="selling_price">120000</field>
            <field name="bedrooms">10</field>
            <field name="living_area">10</field>
            <field name="facades">4</field>
            <field name="is_garage">False</field>
            <field name="is_garden">True</field>
            <field name="property_type_id" ref="estate.estate_property_residential" />
        </record>
        <record id="estate_property_offer_1" model="estate.property.offer">
            <field name="price">10000</field>
            <field name="validity">14</field>
            <field name="property_id" ref="estate.estate_property_big_villa"></field>
            <field name="partner_id" ref="base.res_partner_10"></field>
        </record>
        <record id="estate_property_offer_2" model="estate.property.offer">
            <field name="price">1500000</field>
            <field name="validity">14</field>
            <field name="property_id" ref="estate.estate_property_big_villa"></field>
            <field name="partner_id" ref="base.res_partner_10"></field>
        </record>
        <record id="estate_property_offer_3" model="estate.property.offer">
            <field name="price">1500001</field>
            <field name="validity">14</field>
            <field name="property_id" ref="estate.estate_property_big_villa"></field>
            <field name="partner_id" ref="base.res_partner_12"></field>
        </record>

        <record id="estate_property_partner_1" model="res.partner">
            <field name="name">Azure Interrior</field>
        </record>

        <record id="estate_property_partner_2" model="res.partner">
            <field name="name">Deco Addict</field>
        </record>

        <record id="estate_property_space_ship" model="estate.property">
            <field name="name">International Space Station</field>
            <field name="image" type="base64" file="estate/static/src/img/image3.jpg" />
            <field name="state">new</field>
            <field name="description">Aliens sometimes come visit</field>
            <field name="postcode">123456</field>
            <field name="date_availability">2030-12-31</field>
            <field name="expected_price">45890000</field>
            <field name="property_type_id" ref="estate.estate_property_industrial" />
            <field name="offer_ids"
                eval="[Command.create({
                'price': 7000,
                'partner_id': ref('estate_property_partner_1')
            }),
            Command.create({
                'price': 80000,
                'validity': 50,
                'partner_id': ref('estate_property_partner_2')
            })]"></field>
        </record>
    </data>
</odoo>