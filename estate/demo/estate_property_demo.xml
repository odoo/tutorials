<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!-- Properties -->
    <record id="property_demo_1" model="estate.property">
        <field name="name">Modern Apartment in City Center</field>
        <field name="description">A spacious apartment with modern amenities.</field>
        <field name="postcode">12345</field>
        <field name="expected_price">500000</field>
        <field name="living_area">120</field>
        <field name="facades">2</field>
        <field name="garage" eval="True"/>
        <field name="garden" eval="False"/>
        <field name="property_type_id" ref="property_type_apartment"/>
        <field name="tag_ids" eval="[(6, 0, [ref('property_tag_luxury'), ref('property_tag_furnished')])]"/>
        <field name="salesperson_id" ref="base.user_admin"/>
        <field name="image" type="base64" file="estate/static/src/img/img_1.jpeg"/>
    </record>

