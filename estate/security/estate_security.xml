<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="estate_property_category" model="ir.module.category">
        <field name="name">Estate Category</field>
        <field name="description">This is group</field>
    </record>

    <record id="estate_property_group_agent" model="res.groups">
        <field name="name">Agent</field>
        <field name="category_id" ref="estate.estate_property_category"/>
    </record>

    <record id="estate_property_group_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="estate.estate_property_category"/>
        <field name="implied_ids" eval="[(4,ref('estate_property_group_agent'))]"></field>
    </record>
</odoo>
