<odoo>
    <record id="estate_property_type_action" model="ir.actions.act_window">
        <field name="name">Property Types</field>
        <field name="res_model">estate.property.types</field>
        <field name="view_mode">list,form</field>
    </record>




    <record id="view_estate_property_type_list" model="ir.ui.view">
        <field name="name">estate.property.types.list</field>
        <field name="model">estate.property.types</field>
        <field name="arch" type="xml">
            <list string="Property Types">
                <field name="name"/>
            </list  >
        </field>
    </record>


    <record id="estate_properties_types_form" model="ir.ui.view">
        <field name="name">estate.property.types.form</field>
        <field name="model">estate.property.types</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Property Types">
                <group>
                    <field name="name"/>
                </group>
            </form>
    </field>

    </record>
</odoo>