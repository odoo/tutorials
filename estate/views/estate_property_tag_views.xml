<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="estate_tag_action" model="ir.actions.act_window">
        <field name="name">Estate Property Tag</field>
        <field name="res_model">estate.property.tag</field>
        <field name="view_mode">list,form</field>
    </record>

    <record id="estate_tag_view_tree" model="ir.ui.view">
        <field name="name">estate.property.tag.list</field>
        <field name="model">estate.property.tag</field>
        <field name="arch" type="xml">
            <list string="Channel" editable="bottom">
                <field name="name" />
            </list>
        </field>
    </record>

    <record id="estate_tag_view_form" model="ir.ui.view">
        <field name="name">estate.property.tag.form</field>
        <field name="model">estate.property.tag</field>
        <field name="arch" type="xml">
            <form string="Estate Property Tag">
                <sheet>
                    <field name="name" />
                </sheet>
            </form>
        </field>
    </record>

</odoo>