<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="property_tag_action" model="ir.actions.act_window">
        <field name="name">Property Tags</field>
        <field name="res_model">x_estate.property.tag</field>
        <field name="view_mode">list</field>
    </record>

    <record id="estate_property_tag_view_list" model="ir.ui.view">
        <field name="name">estate.property.tag.view.list</field>
        <field name="model">x_estate.property.tag</field>
        <field name="arch" type="xml">
            <list>
                <field name="x_name"/>
            </list>
        </field>
    </record>
</odoo>
