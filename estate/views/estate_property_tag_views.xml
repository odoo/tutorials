<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="estate_property_tag_action" model="ir.actions.act_window">
        <field name="name">Property Tags</field>
        <field name="res_model">estate.property.tag</field>
        <field name="view_mode">list,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Define new property tag
            </p>
            <p>
                Use options specified in form to list property tag
            </p>
        </field>
    </record>

    <record id="estate_property_tag_view" model="ir.ui.view">
        <field name="name">estate.property.tag.list</field>
        <field name="model">estate.property.tag</field>
        <field name="arch" type="xml">
            <list string="channel" editable="bottom">
                <field name="name" string="property tag Name"></field>
            </list>
        </field>

    </record>

</odoo>
