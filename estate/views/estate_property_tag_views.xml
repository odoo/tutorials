<?xml version="1.0"?>
<odoo>
    <record id="x_estate_property_tag_action" model="ir.actions.act_window">
        <field name="name">Property Tag</field>
        <field name="res_model">x_estate.property.tag</field>
        <field name="view_mode">list</field>
    </record>

    <record id="x_estate_property_tag_list_view" model="ir.ui.view">
        <field name="name">x_estate.property.tag.list</field>
        <field name="model">x_estate.property.tag</field>
        <field name="arch" type="xml">
            <list string="Tag">
                <field name="x_name"/>
            </list>
        </field>
    </record>
</odoo>
