<?xml version="1.0"?>
<odoo>
    <record id="view_estate_model_list" model="ir.ui.view">
        <field name="name">estate.model.list.view</field>
        <field name="model">estate_model</field>
        <field name="arch" type="xml">
            <list string="Estate Properties">
                <field name="name"/>
                <field name="postcode"/>
                <field name="date_availability"/>
                <field name="expected_price"/>
                <field name="selling_price"/>
                <field name="bedrooms"/>
                <field name="living_area"/>
                <field name="facades"/>
                <field name="garage"/>
                <field name="garden"/>
                <field name="garden_area"/>
                <field name="lead_type"/>
                <field name="state"/>
                <field name="active"/>
            </list>
        </field>
    </record>

</odoo>