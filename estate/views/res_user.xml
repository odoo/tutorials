<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_users_view_form_inherit" model="ir.ui.view">
        <field name="name">res.users.form.inherit.estate</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='preferences']" position="after">
                <page name="real_estate" string="Real Estate Properties">
                    <group>
                        <field name="property_ids" readonly="1" nolabel="1"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
