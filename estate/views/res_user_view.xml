<odoo>
        <record id="view_users_form_inherit_estate" model="ir.ui.view"> 
        <field name="name">res.users.form.inherit.estate</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>  <!-- this refers to the existing view-->
        <field name="arch" type="xml">
            <xpath expr="//sheet/notebook" position="inside">
             <page string="Properties">
                <field name="property_res_ids">
                    <list>
                        <field name="name"></field>
                        <field name="expected_price"></field>
                        <field name="status"></field>
                    </list>
                </field>
             </page>
            </xpath>          
        </field>
    </record>
</odoo>