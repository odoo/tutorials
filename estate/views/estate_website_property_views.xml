<?xml version="1.0" encoding="utf-8"?>
<odoo>
   <template id="template_property_list_website_view" name="Property List">
        <t t-call="website.layout">
        <div class="oe_structure">
            <div class="container">
                <br />
                    <h3>Property List</h3>
                <br />
                <div class="flex">
                        <t t-foreach="records" t-as="property">
                            <div>
                                <h3>
                                    <span t-esc="property.name" />
                                </h3>
                                <strong>
                                    Rs. <span t-esc="property.expected_price" />
                                </strong>
                            </div>
                        </t>
                    </div>
            </div>
        </div>
        </t>
   </template>
</odoo>
