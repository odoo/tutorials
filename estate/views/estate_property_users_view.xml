<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="estate_property_users_action" model="ir.actions.act_window">
        <field name="name">Users</field>
        <field name="res_model">res.users</field>
        <field name="view_mode">list,form,kanban,pivot,graph</field>
    </record>
    <record id="users_form_inherited" model="ir.ui.view">
        <field name="name">res.users.inherit.form</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_simple_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='mobile']" position="after">
                <notebook>
                    <page name="real_estate_properties" string="Real Estate Properties">
                        <field name="property_ids"></field>
                    </page>
                </notebook>
            </xpath>
        </field>
    </record>
</odoo>
