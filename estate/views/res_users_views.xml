<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="res_users_views_form" model="ir.ui.view">
        <field name="name">res.users.views.form.inherit.properties</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='account_security']" position="after">
                <page name="Estate Properties" position="before">
                    <field name="property_ids"/>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
