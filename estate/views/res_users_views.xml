<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="eres_users_view_form" model="ir.ui.view">
        <field name="name">estate.property.user.form</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"></field>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page name="properties_assigned" string="Real Estate Properties">
                    <field name="property_ids" />
                </page>
            </xpath>
        </field>
    </record>
</odoo>
