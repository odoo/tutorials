<odoo>

<record id="view_seller_list" model="ir.ui.view">
    <field name="name">estate.property.seller.list</field>
    <field name="model">estate.property.seller</field>
    <field name="arch" type="xml">
        <list string="product_type">
            <field name="name"></field>
        </list>
    </field>
</record>
<record id="view_type_form" model="ir.ui.view">
    <field name="name">estate.property.seller.form</field>
    <field name="model">estate.property.seller</field>
    <field name="arch" type="xml">
    <form string="Product Type">
    <sheet>
        <group>
            <field name="name"></field>
        </group>
    </sheet>
    </form>
    </field>
</record>

<record id="estate_property_seller_action" model="ir.actions.act_window">
    <field name="name">Seller</field>
    <field name="res_model">estate.property.seller</field>
    <field name="view_mode">list,form</field>
</record>
</odoo>