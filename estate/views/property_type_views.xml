<odoo>
    <record id="property_type_action" model="ir.actions.act_window">
        <field name="name">Property Types</field>  <!-- Name to show in the UI -->
        <field name="res_model">property.type</field>  <!-- Model name should be correct -->
        <field name="view_mode">list,form,search</field>
    </record>
    <!-- property type form view -->
    <record id="property_type_view_form" model="ir.ui.view">
        <field name="name">property.type.form</field>
        <field name="model">property.type</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <!--                        <list>-->
                        <!--                            <field name="property_ids" />-->
                        <!--                        </list>-->
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
