<odoo>
    <menuitem id="estate_property_root_menu" name="Estate Property" web_icon="estate,static/src/img/icon.png">
        <menuitem id="estate_property_advertisements_menu" name="Advertisements">
            <menuitem id="estate_property_menu_action" name="Properties"
                action="estate_property_action" />
        </menuitem>
        <menuitem id="estate_property_settings_menu" name="Settings"
            groups="estate.estate_group_manager">
            <menuitem id="estate_property_type_menu" name="Property Types"
                action="estate_property_type_action" />
            <menuitem id="estate_property_tag_menu" name="Property Tags"
                action="estate_property_tag_action" />
        </menuitem>
    </menuitem>
    <record id="menu_estate_property" model="website.menu">
        <field name="name">Estate Property</field>
        <field name="url">/real_estate</field>
        <field name="parent_id" ref="website.main_menu"/>
        <field name="sequence">10</field>
    </record>
</odoo>
