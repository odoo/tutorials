<?xml version="1.0" encoding="utf-8"?>
<odoo>

     <record id="view_property_type_form" model="ir.ui.view">
        <field name="name">estate_property.type.form</field>
        <field name="model">estate_property.type</field>
        <field name="arch" type="xml">
            <form string="Property Form">
                <group>
                        <h1 class= "h1">
                            <field name="name" />
                        </h1>
                </group>
            </form>
        </field>
    </record>

    <record id="estate_property_type_action" model="ir.actions.act_window">
        <field name="name">Properties Type</field>
        <field name="res_model">estate_property.type</field>
        <field name="view_mode">list,form</field>
    </record>




</odoo>
