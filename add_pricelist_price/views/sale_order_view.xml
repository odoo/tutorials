<odoo>
    <record id="view_order_form_inherit_pricelist" model="ir.ui.view">
        <field name="name">view.order.form.inherit.pricelist</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='order_line']/list/field[@name='product_uom_qty']" position="before">
                <field name="book_price" />
            </xpath>
        </field>
    </record>
</odoo>
