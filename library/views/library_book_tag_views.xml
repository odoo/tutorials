<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="library_book_tag_action" model="ir.actions.act_window">
        <field name="name">Book Tag</field>
        <field name="res_model">library.book.tag</field>
        <field name="view_mode">list</field>
    </record>

    <record id="library_book_tag_view_list" model="ir.ui.view">
        <field name="name">library.book.tag.view.list</field>
        <field name="model">library.book.tag</field>
        <field name="arch" type="xml">
            <list string="Book Tag" editable="bottom">
                <field name="name"/>
            </list>
        </field>
    </record>

</odoo>
