<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="book_price.view_order_form" model="ir.ui.view">
        <field name="name">sale.order.view.form.inherit.sale</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='order_lines']//list//field[@name='product_uom_qty']" position="before">
                <field name="pricelist_price_unit"/>
            </xpath>
        </field>
    </record>
</odoo>
