<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="list_warranty_list_view" model="ir.ui.view">
        <field name="model">product.list.product</field>
        <field name="name">product.list.product</field>

        <field name="arch" type="xml">
            <list editable="top">
                <field name="product_id" />
                <field name="qty"/>
                <field name="price"/>
                <field name="order_line_id" column_invisible="1"/>




            </list>
        </field>
    </record>

</odoo>
