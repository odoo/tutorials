<odoo>
    <record id="ir_cron_check_auction_end" model="ir.cron">
        <field name="name">Check Auction End Times and Update Highest Bidder</field>
        <field name="model_id" ref="model_estate_property"/>
        <field name="state">code</field>
        <field name="code">model.check_auction_end()</field>
        <field name="interval_number">5</field>
        <field name="interval_type">minutes</field>
        <field name="nextcall" eval="(DateTime.now() + timedelta(minutes=1)).strftime('%Y-%m-%d %H:%M:%S')" />
       <field name="active" eval="True"/>
    </record>
</odoo>
