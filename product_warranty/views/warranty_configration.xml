<odoo>
  <record id="warranty_configuration_action" model="ir.actions.act_window">
    <field name="name">Product Warranty</field>
    <field name="res_model">product.warranty</field>
    <field name="view_mode">list,form</field>
  </record>

  <record id="warranty_configuration_list_view" model="ir.ui.view">
    <field name="name">warranty_configuration_list_view</field>
    <field name="model">product.warranty</field>
    <field name="arch" type="xml">
      <list>
        <field name="name"></field>
        <field name="product_template_id"></field>
        <field name="percentage"></field>
      </list>
    </field>
  </record>
</odoo>
