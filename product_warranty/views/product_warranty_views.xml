<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- warranty action  -->
    <record id="product_warranty_action" model="ir.actions.act_window">
        <field name="name">product warranty</field>
        <field name="res_model">product.warranty.configuration</field>
        <field name="view_mode">list</field>
    </record>

    <!-- list view -->
    <record id="product_warranty_list_view" model="ir.ui.view">
        <field name="name">product.warranty.configuration</field>
        <field name="model">product.warranty.configuration</field>
        <field name="arch" type="xml">
            <list string="Channel" editable="top">
                <field name="name" string="name" />
                <field name="percentage" string="percentage" />
                <field name="years" string="years" />
                <field name="product_template_id" string="product name"/>



            </list>
        </field>
    </record>

</odoo>
