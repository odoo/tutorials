<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="list_warranty_list_view" model="ir.ui.view">
        <field name="model">product.list.warranty</field>
        <field name="name">product.list.warranty</field>

        <field name="arch" type="xml">
            <list editable="top">
                <field name="product_id" options="{'no_create': true, 'no_open': true}"/>
                <field name="warranty_id" options="{'no_create': true, 'no_open': true}"/>
                <field name="qty"/>
                <field name="enddate"/>
                <field name="order_line_id" />



            </list>
        </field>
    </record>

</odoo>
