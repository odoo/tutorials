<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="sales_order_view_inherit" model="ir.ui.view">
        <field name="name">inherit.sale.order.view</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='order_line']/list/field[@name='product_uom_qty']" position="before">
                <field name="book_price" readonly="1" />
            </xpath>
        </field>
    </record>
</odoo>