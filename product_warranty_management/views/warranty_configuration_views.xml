<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="warranty_configuration_list_view" model="ir.ui.view">
        <field name="name">warranty_configuration_list_view</field>
        <field name="model">warranty.configuration</field>
        <field name="arch" type="xml">
            <list editable="bottom">
                <field name="name"/>
                <field name="validity"/>
                <field name="product_id"/>
                <field name="percentage"/>
            </list>
        </field>
    </record>
    <record id="warranty_configuration_action" model="ir.actions.act_window">
        <field name="name">warranty_configuration_action</field>
        <field name="res_model">warranty.configuration</field>
        <field name="view_mode">list</field>
    </record>
</odoo>
