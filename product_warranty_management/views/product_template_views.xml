<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="product_template_form_view_inherit_product" model="ir.ui.view">
        <field name="name">product.template.form.view.inherit.product</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_tag_ids']" position="after">
                <field name="is_warranty_available"/>
            </xpath>
        </field>
    </record>
</odoo>
