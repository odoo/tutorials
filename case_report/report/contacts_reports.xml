<odoo>
    <record id="report_contacts" model="ir.actions.report">
        <field name="name">Contacts</field>
        <field name="model">res.partner</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">case_report.contacts_report_template</field>
