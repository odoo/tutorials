<?xml version="1.0"?>
<odoo>
    <record id="real_estate_property_type" model="ir.actions.act_window">
        <field name="name">Property Type</field>
        <field name="res_model">real.estate.property.type</field>
        <field name="view_mode">list,form</field>
    </record>

    <!-- Form View -->
    <record id="real_estate_property_form" model="ir.ui.view">
        <field name="name">real.estate.property.type.form</field>
        <field name="model">real.estate.property.type</field>
        <field name="arch" type="xml">
            <form string="Property Form">
                <sheet>
                    <group>
                        <h1>
                            <field name="name"/>
                        </h1>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
