<odoo>
    <!-- Property Offer Report -->
    <record id="action_report_property_offers" model="ir.actions.report">
        <field name="name">Property Offer Report</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">real_estate.report_property_offers</field>
        <field name="report_file">real_estate.report_property_offers</field>
        <field name="binding_model_id" ref="model_estate_property"/>
        <field name="binding_type">report</field>
    </record>

    <!-- User Real Estate Report -->
    <record id="action_report_res_users_estate" model="ir.actions.report">
        <field name="name">Salesman Estate Report</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_res_users_estate_template</field>
        <field name="report_file">real_estate.report_res_users_estate_template</field>
        <field name="binding_model_id" ref="base.model_res_users"/>
        <field name="binding_type">report</field>
    </record>
</odoo>