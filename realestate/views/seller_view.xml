<odoo>


    <record id="seller_action" model="ir.actions.act_window">
        <field name="name">Seller</field>
        <field name="res_model">seller</field>
        <field name="view_mode">list,form</field>
    </record>

    <menuitem id="real_estate_root_menu" name="Real Estate">
        <menuitem id="Users_menu" name="Users">
            <menuitem id="seller_menu_item" name="Sellers" action="seller_action" />
        </menuitem>
    </menuitem>

    <record id='seller_form_view' model='ir.ui.view'>
        <field name='name'>Seller</field>
        <field name='model'>seller</field>
        <field name='arch' type='xml'>
            <form string='Properties'>
                <sheet>
                    <notebook>

                        <page string="Properties">
                            <field name="properties_ids" />

                        </page>


                    </notebook>


                </sheet>


            </form>
        </field>


    </record>


</odoo>