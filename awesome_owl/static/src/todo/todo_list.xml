<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="awesome_owl.todo_list">
        <div class="card w-25">
            <div class="card-body ">
                <t t-foreach="todos" t-as="todo" t-key="todo.id">
                    <TodoItem todo="todo" />
                </t>
            </div>
        </div>
    </t>
</templates>