<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
    <t t-name="awesome_owl.TodoList">
        <div class="card w-50 p-2">
            <h1>TodoList</h1>
            <t t-foreach="todos" t-as="todo" t-key="todo.id">
                <TodoItem todo="todo"/>
            </t>
        </div>
    </t>
</templates>
