<?xml version="1.0" encoding="utf-8" ?>
<templates xml:space="preserve">
   <t t-name="awesome_owl.todo_list">
      <div class="card p-3">
         <h3>Todo List</h3>
         <div>
            <t t-foreach="todos" t-as="todo" t-key="todo.id">
               <TodoItem todo="todo"/>
            </t>
         </div>
      </div>
   </t>
</templates>
