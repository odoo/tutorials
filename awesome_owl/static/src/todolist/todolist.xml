<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">

    <t t-name="awesome_owl.ToDoList">
        <div class="card d-inline-block m-2" style="width: 18rem;">
            <t t-foreach="todos" t-as="td" t-key="td.id"> 
                <div class="m-3">   
                    <ToDoItem todo="td"/>
                </div>
            </t>
       </div>
    </t>

</templates>