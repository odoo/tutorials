<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
    <t t-name="awesome_owl.counter">
        <div class="p-3">
            <p>Counter: <t t-esc="count.value"/></p>
            <button class="btn btn-primary" t-on-click="()=>{this.increment(); props.onChange();}">Increase Counter</button>
        </div>
    </t>
</templates>
