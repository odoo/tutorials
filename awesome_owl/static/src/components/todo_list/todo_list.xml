<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
    <t t-name="awesome_owl.TodoList" owl="1">
        <div class="todo-list">
            <h3>Todo List</h3>
            <div>
                <t t-foreach="todos" t-as="todo" t-key="todo.id">
                    <TodoItem todo="todo"/>
                </t>
            </div>
        </div>
    </t>
</templates>
