<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="view_order_form_inherit_stancor_customization" model="ir.ui.view">
    <field name="name">view.order.form.inherit.stancor.customization</field>
    <field name="model">sale.order</field>
    <field name="inherit_id" ref="sale.view_order_form"/>
    <field name="arch" type="xml">
      <xpath expr="//list/field[@name='product_uom_qty']" position="after">
        <field name="s_quantity"/>
        <field name="s_unit"/>
      </xpath>
    </field>
  </record>
</odoo>