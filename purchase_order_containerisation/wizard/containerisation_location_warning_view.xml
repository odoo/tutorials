<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_containerisation_location_warning_form" model="ir.ui.view">
        <field name="name">containerisation.location.warning.form</field>
        <field name="model">containerisation.location.warning</field>
        <field name="arch" type="xml">
            <form string="Location Mismatch Warning">
                <group>
                    <strong><field name="warning_message" nolabel="1"/></strong>
                </group>
                <footer>
                    <button name="action_continue" string="Continue" type="object" class="oe_highlight"/>
                    <button name="action_cancel" string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
