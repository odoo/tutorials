<?xml version="1.0"?>
<odoo>
    <record id="estate_property_tags_view_list" model="ir.ui.view">
        <field name="name">estate.property.tags.view.list</field>
        <field name="model">estate.property.tags</field>
        <field name="arch" type="xml">
            <list string="Tags to Real Estate Properties" editable="bottom">
                <field name="name"/>
            </list>
        </field>
    </record>

    <record id="estate_property_tags_action" model="ir.actions.act_window">
        <field name="name">Property Tags Main Action</field>
        <field name="res_model">estate.property.tags</field>
        <field name="view_mode">list,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
            Define tags for real estate property
            </p>
        </field>
    </record>
</odoo>